<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>Todo List</title>
    <link rel="icon" href="/assets/favicon.ico"/>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <body>
        <div id="root"></div>
    <% if(process.env.NODE_ENV === 'production') { %>
        <script crossorigin src="/assets/scripts/react.production.min.js"></script>
        <script crossorigin src="/assets/scripts/react-dom.production.min.js"></script>
    <% } else { %>
        <script crossorigin src="/assets/scripts/react.development.js"></script>
        <script crossorigin src="/assets/scripts/react-dom.development.js"></script>
		<% } %>
        <% for(var jsLocation of Object.values(manifest)) {%>
        <script type="text/javascript" src="<%- jsLocation %>"></script>
        <% } %>
    </body>
</html>
